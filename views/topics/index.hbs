<a href="/topics/new">New Topic</a>
<!-- Iterates through each author -->
{{#each users}}
  <div class="topic">
    <div class="topicName">{{name}}</div>
    <!-- For each topic, it will iterate through their articles -->
    {{#each topics}}
      <p class='reminder'>{{keyword}}</p>
    {{/each}}
  </div>
{{/each}}
